<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage
    x:Class="Cultris_II.Views.Navigation.FP"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:nav="clr-namespace:Cultris_II.Views.Navigation"
    xmlns:pages="clr-namespace:Cultris_II.Views"
    NavigationPage.HasBackButton="False"
    NavigationPage.HasNavigationBar="False">
    <FlyoutPage.Flyout>
        <nav:FPFlyout x:Name="FlyoutPage" />
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <pages:RoomsPage />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>